generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("URL")
}

model Contact {
  id               Int      @id @default(autoincrement())
  email            String   @unique
  phone            String?  @unique
  linkedID         Int
  linkedPrecedince String   @default("primary") // it can be secondary
  createdAt        DateTime @default(now())
  updatedAt        DateTime @updatedAt
}
